class Solution:
    def minDeletionSize(self, strs):
        n = len(strs)
        m = len(strs[0])

        # dp[j] = longest valid sequence ending at column j
        dp = [1] * m

        for j in range(m):
            for i in range(j):
                # Check if column i can come before column j
                valid = True
                for row in range(n):
                    if strs[row][i] > strs[row][j]:
                        valid = False
                        break

                if valid:
                    dp[j] = max(dp[j], dp[i] + 1)

        # We keep the longest valid sequence of columns
        return m - max(dp)
