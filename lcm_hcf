
def gcd(a, b):
    while b != 0:
        temp = b
        b = a % b
        a = temp
    return a
num1, num2 = map(int, input("Enter two numbers: ").split())
hcf = gcd(num1, num2)
lcm = (num1 * num2) // hcf
print("HCF of", num1, "and", num2, "is:", hcf)
print("LCM of", num1, "and", num2, "is:", lcm)
